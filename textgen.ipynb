    "import random\n",
    "import string\n",
    "\n",
    "def generate_random_text(length=6):\n",
    "    characters = string.ascii_letters + string.digits  # Includes uppercase, lowercase, and digits\n",
    "    return ''.join(random.choices(characters, k=length))\n",
    "\n",
    "# Example usage\n",
    "print(generate_random_text())"
   ]
